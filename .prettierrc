{
    "singleQuote": true,
    "trailingComma": "all",
    "semi": true,
    "arrowParens": "avoid",
    "tabWidth": 4,
    "printWidth": 100,
    "importOrder": ["^@nestjs/(.*)$", "<THIRD_PARTY_MODULES>", "^[./]"],
    "importOrderSortSpecifiers": true,
    "importOrderCaseInsensitive": true,
    "importOrderParserPlugins": ["typescript", "decorators-legacy"]
}
